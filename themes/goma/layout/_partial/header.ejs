<header class="header">
  <div class="header__inner row">
    <div class="s-12 m-4">
      <h1 class="header__logo">
        <a href="<%- url_for() %>"><%= config.title %></a>
      </h1>
    </div>
    <div class="s-12 m-8">
      <nav class="nav">
        <ul class="nav__menu">
          <% for (var i in theme.menu){ %>
            <li>
              <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
            </li>
          <% } %>
        </ul>
      </nav>
    </div>
  </div>
</header>